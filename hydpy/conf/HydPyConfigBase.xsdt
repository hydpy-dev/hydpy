<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="https://github.com/hydpy-dev/hydpy/releases/download/your-hydpy-version/HydPyConfigBase.xsd"
        xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:hpcb="https://github.com/hydpy-dev/hydpy/releases/download/your-hydpy-version/HydPyConfigBase.xsd"
        elementFormDefault="qualified">

    <simpleType name="listOfStrings">
      <list itemType="string"/>
    </simpleType>

    <simpleType name="nonEmptyListOfStrings">
      <restriction base="string">
          <minLength value="1"/>
      </restriction>
    </simpleType>

    <simpleType name="listOfDoubles">
      <list itemType="double"/>
    </simpleType>

    <element name="options">
        <complexType>
            <choice maxOccurs="unbounded">
                <element name="checkseries"
                         type="boolean"
                         minOccurs="0"/>
                <element name="parameterstep"
                         minOccurs="0">
                    <simpleType>
                        <restriction base="string">
                            <pattern value="[0-9]+[smhd]"/>
                        </restriction>
                    </simpleType>
                </element>
                <element name="reprdigits"
                         minOccurs="0">
                    <simpleType>
                        <restriction base="integer">
                            <minInclusive value="-1"/>
                            <maxInclusive value="12"/>
                        </restriction>
                    </simpleType>
                </element>
                <element name="usecython"
                         type="boolean"
                         minOccurs="0"/>
                <element name="usedefaultvalues"
                         type="boolean"
                         minOccurs="0"/>
                <element name="utcoffset"
                         type="integer"
                         minOccurs="0"/>
                <element name="timestampleft"
                         type="boolean"
                         minOccurs="0"/>
                <element name="warnmissingobsfile"
                         type="boolean"
                         minOccurs="0"/>
                <element name="warnmissingsimfile"
                         type="boolean"
                         minOccurs="0"/>
                <element name="warntrim"
                         type="boolean"
                         minOccurs="0"/>
            </choice>
        </complexType>
    </element>

    <element name="timegrid">
        <complexType>
            <sequence>
                <element name="firstdate" type="dateTime"/>
                <element name="lastdate" type="dateTime"/>
                <element name="stepsize">
                    <simpleType>
                        <restriction base="string">
                            <pattern value="[0-9]+[smhd]"/>
                        </restriction>
                    </simpleType>
                </element>
            </sequence>
        </complexType>
    </element>

    <element name="add_selections">
        <complexType>
            <sequence>
                <element ref="hpcb:add_selection" maxOccurs="unbounded"/>
            </sequence>
        </complexType>
    </element>

    <element name="add_selection">
        <complexType>
            <sequence>
                <element name="name"
                     type="string"/>
                <element ref="hpcb:devices"
                         minOccurs="0"/>
                <element ref="hpcb:keywords"
                         minOccurs="0"/>
                <element ref="hpcb:selections"
                         minOccurs="0"/>
            </sequence>
        </complexType>
    </element>

    <element name="selections"
             type="hpcb:nonEmptyListOfStrings"/>

    <element name="devices"
             type="hpcb:nonEmptyListOfStrings"/>

    <element name="keywords"
             type="hpcb:nonEmptyListOfStrings"/>

    <element name="network_io"
             type="hpcb:nonEmptyListOfStrings"/>

    <element name="control_io"
             type="hpcb:nonEmptyListOfStrings"/>

    <element name="conditions_io">
        <complexType>
            <sequence>
                <element name="inputdir"
                         type="anyURI"/>
                <element name="outputdir"
                         type="anyURI"/>
                <element name="zip"
                         type="boolean"/>
            </sequence>
        </complexType>
    </element>

    <simpleType name="mode">
        <restriction base="string">
            <enumeration value="ram"/>
            <enumeration value="jit"/>
        </restriction>
    </simpleType>


    <element name="series_io">
        <complexType>
            <sequence>
                <element name="directory"
                         type="string"
                         minOccurs="0"/>
                <element name="filetype"
                         minOccurs="0"
                         type="hpcb:filetype"/>
                <element name="overwrite"
                         minOccurs="0"
                         type="boolean"/>
                <element name="aggregation"
                         minOccurs="0"
                         type="hpcb:aggregation"/>
                <element name="mode"
                         minOccurs="0"
                         type="hpcb:mode"/>
                <element name="readers">
                    <complexType>
                        <sequence>
                            <element ref="hpcb:reader"
                                     maxOccurs="unbounded"/>
                        </sequence>
                    </complexType>
                </element>
                <element name="writers">
                    <complexType>
                        <sequence>
                            <element ref="hpcb:writer"
                                     minOccurs="0"
                                     maxOccurs="unbounded" />
                        </sequence>
                    </complexType>
                </element>
            </sequence>
        </complexType>
    </element>

    <element name="reader">
        <complexType>
            <sequence>
                <element name="directory"
                         type="string"
                         minOccurs="0"/>
                <element ref="hpcb:selections"
                         minOccurs="0"/>
                <element name="sequences"
                         type="hpcb:readerType"/>
                <element name="filetype"
                         minOccurs="0"
                         type="hpcb:filetype"/>
                <element name="mode"
                         minOccurs="0"
                         type="hpcb:mode"/>
            </sequence>
            <attribute name="info" type="string"/>
        </complexType>
    </element>

    <element name="writer">
        <complexType>
            <sequence>
                <element name="directory"
                         type="string"
                         minOccurs="0"/>
                <element ref="hpcb:selections"
                         minOccurs="0"/>
                <element name="sequences"
                         type="hpcb:writerType"/>
                <element name="filetype"
                         minOccurs="0"
                         type="hpcb:filetype"/>
                <element name="overwrite"
                         minOccurs="0"
                         type="boolean"/>
                <element name="aggregation"
                         minOccurs="0"
                         type="hpcb:aggregation"/>
                <element name="mode"
                         minOccurs="0"
                         type="hpcb:mode"/>
            </sequence>
            <attribute name="info" type="string"/>
        </complexType>
    </element>

    <simpleType name="filetype">
        <restriction base="string">
            <enumeration value="asc"/>
            <enumeration value="npy"/>
            <enumeration value="nc"/>
        </restriction>
    </simpleType>

    <simpleType name="aggregation">
        <restriction base="string">
            <enumeration value="none"/>
            <enumeration value="mean"/>
        </restriction>
    </simpleType>

    <complexType name="node_readerType">
        <sequence>
            <element name="sim"
                     minOccurs="0"/>
            <element name="obs"
                     minOccurs="0"/>
        </sequence>
    </complexType>

    <complexType name="node_writerType">
        <sequence>
            <element name="sim"
                     minOccurs="0"/>
            <element name="obs"
                     minOccurs="0"/>
        </sequence>
    </complexType>

<!--include model sequence groups-->

    <element name="exchange">
        <complexType>
            <sequence>
                <element ref="hpcb:setitems"
                         minOccurs="0"
                         maxOccurs="unbounded"/>
                <element ref="hpcb:additems"
                         minOccurs="0"
                         maxOccurs="unbounded"/>
                <element ref="hpcb:multiplyitems"
                         minOccurs="0"
                         maxOccurs="unbounded"/>
                <element ref="hpcb:getitems"
                         minOccurs="0"
                         maxOccurs="unbounded"/>
            </sequence>
        </complexType>
    </element>

    <simpleType name="_levelType">
        <restriction base="string">
            <enumeration value="global"/>
            <enumeration value="selection"/>
            <enumeration value="device"/>
            <enumeration value="subunit"/>
        </restriction>
    </simpleType>

    <complexType name="setitemType">
        <sequence>
            <element ref="hpcb:selections"
                     minOccurs="0"/>
            <element name="name"
                     type="string"/>
            <element name="level"
                     type="hpcb:_levelType"/>
            <element name="init"
                     type="hpcb:listOfDoubles"
                     minOccurs="0"/>
        </sequence>
    </complexType>

    <complexType name="mathitemType">
        <sequence>
            <element ref="hpcb:selections"
                     minOccurs="0"/>
            <element name="name"
                     type="string"/>
            <element name="level"
                     type="hpcb:_levelType"/>
            <element name="init"
                     type="hpcb:listOfDoubles"/>
        </sequence>
    </complexType>

    <complexType name="getitemType">
        <sequence>
            <element name="name"
                     type="string"
                     minOccurs="0"/>
            <element ref="hpcb:selections"
                     minOccurs="0"/>
        </sequence>
    </complexType>

<!--include exchange items-->

</schema>
