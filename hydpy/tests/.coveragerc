
[run]
source =
    hydpy
omit =
    */hydpy/tests/*
    */hydpy/docs/*
    */hydpy/cythons/modelutils.py
parallel=True

[report]
show_missing = True
exclude_lines =
    pragma: no cover
    if TYPE_CHECKING
    if typing.TYPE_CHECKING
    ^    +\.\.\.
    ^    +pass$
    ^    +pass .*
    assert .*
    ^    +assert_never\(.*\)
